<?xml version="1.0"?>
<GhussakTweaks>
  <append xpath="/gameevents">
    <action_sequence name="eventGSKHelperSwimUp">
      <action class="Teleport">
        <property name="target_position" value="0,1,0"/>
        <property name="offset_type" value="Relative"/>
      </action>
    </action_sequence>
    <action_sequence name="eventGSKHelperSwimDown">
      <action class="Teleport">
        <property name="target_position" value="0,-1,0"/>
        <property name="offset_type" value="Relative"/>
      </action>
    </action_sequence>
  </append>
</GhussakTweaks>
