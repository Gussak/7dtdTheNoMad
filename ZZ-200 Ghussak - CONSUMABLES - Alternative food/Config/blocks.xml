  <!--
	HELPGOOD:CODEGEN:KILLPLANTS:TAGS  (sync with gameevents.xml)
clear;IFS=$'#\n' read -d '' -r -a astrList < <(egrep '"plant[^"]*dead"' -\-include=*.xml * -iRhIao |tr -d '"' |sort -u);\
#astrList+=(plantedGoldenrodDead plantedChrysanthemumDead plantedMushroomDead plantedGraceCornDead plantedPumpkinDead);\
astrList=($(echo "${astrList[@]}" |tr ' ' '\n' |sort -u));\
for strDead in ${astrList[@]};do\
	strAlive="${strDead%Dead}"
	case "${strAlive}" in
		plantHedge|plantShrub) # these have no tags property
			echo -e '	<append xpath="/blocks/block[@name='"'"${strAlive}"'"']"><property name="Tags" value="'${strAlive}'"/></append>'
			;;
		*)
			if [[ "${strAlive}" == plantedGraceCorn ]];then #have no tags property
				echo -e '	<append xpath="/blocks/block[@name='"'"${strAlive}1"'"']"><property name="Tags" value="'${strAlive}'"/></append>'
			else
				echo -e '	<append xpath="/blocks/block[@name='"'"${strAlive}1"'"']/property[@name='"'"Tags"'"']/@value">,'${strAlive}'</append>'
			fi
			echo -e '	<append xpath="/blocks/block[@name='"'"${strAlive}3Harvest"'"']"><property name="Tags" value="'${strAlive}'"/></append>'
			;;
	esac
	# all dead have no tags property
	echo -e '	<append xpath="/blocks/block[@name='"'"${strDead}"'"']"><property name="Tags" value="'${strDead}'"/></append>'
done
-->
