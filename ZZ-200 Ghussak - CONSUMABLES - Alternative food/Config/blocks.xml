  <!--
	HELPGOOD:CODEGEN:KILLPLANTS:TAGS  (sync with gameevents.xml)
clear;IFS=$'#\n' read -d '' -r -a astrList < <(egrep '"plant[^"]*dead"' -\-include=*.xml * -iRhIao |tr -d '"' |sort -u);\
astrList+=(plantedGoldenrodDead plantedChrysanthemumDead plantedMushroomDead plantedGraceCornDead plantedPumpkinDead);\
for strDead in ${astrList[@]};do\
	if [[ 
			"${strDead%Dead}" == plantHedge || 
			"${strDead%Dead}" == plantShrub 
	]];then
		echo -e '	<append xpath="/blocks/block[@name='"'"${strDead%Dead}"'"']"><property name="Tags" value="'${strDead%Dead}'"/></append>'
	else
		echo -e '	<append xpath="/blocks/block[@name='"'"${strDead%Dead}1"'"']/property[@name='"'"Tags"'"']/@value">,'${strDead%Dead}'</append>'
		echo -e '	<append xpath="/blocks/block[@name='"'"${strDead%Dead}3Harvest"'"']"><property name="Tags" value="'${strDead%Dead}'"/></append>'
	fi
	echo -e '	<append xpath="/blocks/block[@name='"'"${strDead}"'"']"><property name="Tags" value="'${strDead}'"/></append>'
done
-->
