<GhussakTweaks>
	<append xpath="/gameevents">
		<action_sequence name="eventGSKadvanceTimeOnce">
			<action class="TimeChange" help="in game minutes">
				<property name="min_time" value="5" />
				<property name="max_time" value="15" />
			</action>
		</action_sequence>
		
		<action_sequence name="eventGSKrewindTimeOnce">
			<action class="TimeChange" help="in game minutes">
				<property name="min_time" value="-60" />
				<property name="max_time" value="-60" />
			</action>
		</action_sequence>
		<action_sequence name="eventGSKrewindTimeOnceX6">
			<action class="TimeChange" help="in game minutes">
				<property name="min_time" value="-360" />
				<property name="max_time" value="-360" />
			</action>
		</action_sequence>
		
	</append>
</GhussakTweaks>
