    <!--
	HELPGOOD:CODEGEN:
	clear;IFS=$'#\n' read -d '' -r -a astrList < <(egrep -oi 'recipe name="gun[^"]*' _NewestSavegamePath.IgnoreOnBackup/ConfigsDump/recipes.xml |egrep -vi "parts|pipe|primitive" |cut -d'"' -f2); \
	for strNm in "${astrList[@]}";do
		strParts=""
		strPerk=""
		for strChk in gunHandgunT1Pistol gunHandgunT2Magnum44 gunHandgunT3DesertVulture gunHandgunT3SMG5;do
			if [[ $strNm == $strChk ]];then strParts=gunHandgunT1PistolParts;strPerk=perkGunslinger;break;fi
		done
		for strChk in gunShotgunT1DoubleBarrel gunShotgunT2PumpShotgun gunShotgunT3AutoShotgun;do
			if [[ $strNm == $strChk ]];then strParts=gunShotgunT1DoubleBarrelParts;strPerk=perkBoomstick;break;fi
		done
		for strChk in gunRifleT1HuntingRifle gunRifleT2LeverActionRifle gunRifleT3SniperRifle;do
			if [[ $strNm == $strChk ]];then strParts=gunRifleT1HuntingRifleParts;strPerk=perkDeadEye;break;fi
		done
		for strChk in gunMGT1AK47 gunMGT2TacticalAR gunMGT3M60;do
			if [[ $strNm == $strChk ]];then strParts=gunMGT1AK47Parts;strPerk=perkMachineGunner;break;fi
		done
		for strChk in gunBowT1WoodenBow gunBowT3CompoundBow gunBowT1IronCrossbow gunBowT3CompoundCrossbow;do
			if [[ $strNm == $strChk ]];then strParts=gunBowT1WoodenBowParts;strPerk=perkArchery;break;fi
		done
		for strChk in gunBotT1JunkSledge gunBotT2JunkTurret gunBotT3JunkDrone;do
			if [[ $strNm == $strChk ]];then strParts=gunBotRoboticsParts;strPerk=perkTurrets;break;fi
		done
		if [[ $strNm == gunExplosivesT3RocketLauncher ]];then
			strPerk=perkDemolitionsExpert;
			strParts=gunExplosivesT3RocketLauncherParts;
		fi
		if [[ -z "$strParts" ]];then read -p "ERROR:$strNm";fi
		nCraftTime=
		if [[ $strNm =~ .*T3.* ]];then nPartsBaseCount=18;nDuctTapes=3;nCraftTime=15;fi #vanilla parts to create 13
		if [[ $strNm =~ .*T2.* ]];then nPartsBaseCount=10;nDuctTapes=2;nCraftTime=10;fi #vanilla parts to create 7
		if [[ $strNm =~ .*T1.* ]];then nPartsBaseCount=2 ;nDuctTapes=1;nCraftTime=5 ;fi #vanilla parts to create 1
		if [[ $strNm =~ gunBotT.Junk.* ]];then nPartsBaseCount=2;fi #vanilla parts to create at any tier 1
		echo '
		<recipe name="'"${strNm}"'" count="1" craft_time="'"${nCraftTime}"'" tags="'"${strPerk}"'" help="Rationalle: nPartsBaseCount and base_add=1to5, being OTG, is more because some parts will be lost by not having a proper wokbench to work with it.">
			<ingredient name="'"${strNm}"'" count="1" />
			<ingredient name="'"${strParts}"'" count="'"${nPartsBaseCount}"'" />
			<ingredient name="resourceRepairKit" count="2" />
			<ingredient name="resourceDuctTape" count="'"${nDuctTapes}"'" />
			<ingredient name="resourceLegendaryParts" count="0" />
			<effect_group>
				<passive_effect name="CraftingIngredientCount" level="2,6" operation="base_add" value="2,10" tags="'"${strParts}"'" />
				<passive_effect name="CraftingIngredientCount" level="6" operation="base_add" value="1" tags="resourceLegendaryParts" />
			</effect_group>
		</recipe>'
	done
-->
